package domain.valueo;

public class Technology extends Value{
	private String technology;	// 部署ID

	/* コンストラクタ */
	public Technology(String technology){
		super();
		this.technology = technology;
	}

	// 部署IDを取得
	public String getTechnology(){
		return technology;
	}

	@Override
	public String toString(){
		return technology;
	}
}
